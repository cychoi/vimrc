let SessionLoad = 1
if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
inoremap <C-F5> :call AddForward()
inoremap <F5> :call AddHeader()
nmap ,lm :call LicenseHeader( "MIT" )
nmap ,ll :call LicenseHeader( "LGPL" )
nmap ,lg :call LicenseHeader( "GPL" )
nmap ,i :call IncludeGuard()
vmap ,u :s,^//X ,,
vmap ,c :s,^,//X ,:noh
nmap <silent> ,p :call SwitchPrivateHeaderImpl()
nmap <silent> ,h :call SwitchHeaderImpl()
map \o :call SetNoCopy()
map \p :call SetCopy()
map \w :call HandleURI()
nmap gx <Plug>NetrwBrowseX
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetrwBrowseX(expand("<cWORD>"),0)
nnoremap <C-Right> :bn
nnoremap <C-Left> :bp
inoremap 	 =SmartTab()
inoremap  =pumvisible() ? "\<CR>" : "\<ESC>:call SmartLineBreak()\<CR>a\<CR>"
inoremap ( =SmartParens( '(' )
inoremap ) =SmartParens( ')', '(' )
inoremap [ =SmartParens( '[' )
inoremap ] =SmartParens( ']', '[' )
iabbr else =SmartElse()
iabbr DIFF :call RunDiff()
iabbr #i =SmartInclude()
let &cpo=s:cpo_save
unlet s:cpo_save
set autoindent
set background=dark
set backspace=indent,eol,start
set directory=/var/tmp,/tmp,.
set expandtab
set fileencodings=ucs-bom,utf-8,latin1
set guicursor=n-v-c:block,o:hor50,i-ci:hor15,r-cr:hor30,sm:block,a:blinkon0,a:blinkon0
set helplang=en
set history=50
set hlsearch
set ignorecase
set incsearch
set laststatus=2
set listchars=tab:·\ ,trail:·
set makeprg=makeobj
set mouse=a
set pastetoggle=<F2>
set path=.,/usr/include,/usr/local/include,~/dev/install/usr//include,~/dev/install/usr//include,/usr/include,,,
set ruler
set scrolloff=6
set shiftwidth=2
set smartcase
set softtabstop=2
set statusline=\ %F%m%r%h\ %w\ \ CWD:\ %{getcwd()}\ \ \ Line:\ %l/%L:%c
set suffixes=.bak,~,.o,.h,.info,.swp,.obj,.lo,.o,.moc,.la,.closure,.loT
set textwidth=100
set viminfo='20,\"50
set window=41
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
